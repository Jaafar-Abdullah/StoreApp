<%= form_for @booking do |f| %>
  <div class="field">
    <%= f.label :start_date %> 
    <%= f.date_field :start_id %>
  </div>
  <div class="field">
    <%= f.label :end_date %> 
    <%= f.date_field :end_date %>
  </div>
  <%= hidden_field_tag "booking[product_id]", @product.id %>

<div><%= f.submit %></div>
<% end %>